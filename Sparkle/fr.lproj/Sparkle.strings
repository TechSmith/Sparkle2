/* Description text for SUUpdateAlert when the update is downloadable. */
"%@ %@ is now available--you have %@. Would you like to download it now?" = "%1$@ %2$@ est désormais disponible. Vous utilisez %3$@. Voulez-vous le télécharger maintenant ?";

/* Description text for SUUpdateAlert when the update informational with no download. */
"%@ %@ is now available--you have %@. Would you like to learn more about this update on the web?" = "%1$@ %2$@ est désormais disponible. Vous utilisez %3$@. Souhaitez-vous consulter des informations en ligne sur cette mise à jour ?";

/*  */
"%@ downloaded" = "%@ téléchargé";

/*  */
"%@ of %@" = "%1$@ sur %2$@";

/*  */
"A new version of %@ is available!" = "Une nouvelle version de %@ est disponible.";

